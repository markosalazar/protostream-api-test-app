<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
       <Console name="STDOUT" target="SYSTEM_OUT">
        	<PatternLayout pattern="%5p %c %d{dd MMM yyyy HH:mm:ss}. - %m%n"/>
    	</Console>
    </Appenders>

    <Loggers>
        <Root level="info">
            <AppenderRef ref="STDOUT" />
        </Root>
    </Loggers>

</Configuration>
